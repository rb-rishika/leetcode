class Solution:
    def removeElement(self, nums: List[int], val: int) -> int:
        i = 0
        index=0
    
    # Traverse through all elements in nums
        while(i<len(nums)):
            # If the current element is not equal to val, we place it at index j
            if nums[i] != val:
                nums[index] = nums[i]
                index += 1
                print(index)
            i+=1
        
        # Return the number of elements that are not equal to val
        return index
        
        
