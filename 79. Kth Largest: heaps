class Solution:
    def findKthLargest(self, nums: List[int], k: int) -> int:
        heapnew=[]
        for i in range(len(nums)):
            heapq.heappush(heapnew, nums[i]) #push inside the heap and pop when you find the size of the heap greater than k
            if (len(heapnew)> k):
                heapq.heappop(heapnew)
        return heapnew[0]
        
